ed in media queries\r\n$breakpoints: (\r\n  \"tablet\": 43em,\r\n  \"laptop\": 62em,\r\n  \"desktop\": 82em,\r\n);\r\n","sass/abstracts/_mixins.scss":"@use \"variables\";\r\n\r\n/*------------------------------*\\\r\n    #mixins \r\n\\*------------------------------*/\r\n\r\n@mixin respond-to($breakpoint) {\r\n  @media screen and (min-width: map-get($map:variables.$breakpoints, $key:$breakpoint )) {\r\n    @content;\r\n  }\r\n}\r\n\r\n@mixin flex-container($al-con: center, $js-con: space-between, $flex-d: row) {\r\n  display: flex;\r\n  align-items: $al-con;\r\n  justify-content: $js-con;\r\n  flex-direction: $flex-d;\r\n}\r\n","sass/base/_reset.scss":"@use \"../abstracts\" as variables;\r\n\r\n/*------------------------------*\\\r\n    #resets\r\n\\*------------------------------*/\r\n\r\n/*|| global rests */\r\n\r\nhtml {\r\n  box-sizing: border-box;\r\n}\r\n\r\n* {\r\n  box-sizing: initial;\r\n  margin: 0;\r\n  padding: 0;\r\n  font-size: inherit;\r\n}\r\n\r\nbody {\r\n  min-height: 100vh;\r\n  overflow-x: none;\r\n}\r\n\r\n/*|| making them fit inside the container */\r\n\r\npicture,\r\nimage {\r\n  max-width: 100%;\r\n}\r\n\r\n/*|| removing the default list style */\r\nul[role=\"list\"],\r\nol[role=\"list\"] {\r\n  list-style: none;\r\n}\r\n\r\na,\r\na:visited {\r\n  text-decoration: none;\r\n}\r\n\r\nbutton {\r\n  border: none;\r\n}\r\n\r\n/*||remove the default styles for a and buttons on focus state but reset them when element is tabed.*/\r\nbutton,\r\na {\r\n  cursor: pointer;\r\n\r\n  &:focus {\r\n    outline: max(1, 0.1em) solid #000;\r\n    outline-offset: -0.25em;\r\n  }\r\n\r\n  &:not(:focus-visible) {\r\n    outline: none;\r\n  }\r\n\r\n  &:focus-visible {\r\n    outline: max(1, 0.1em) solid #000;\r\n    outline-offset: -0.25em;\r\n  }\r\n}\r\n\r\n/*|| Remove all animations and transitions\r\nfor people that prefer not to see them */\r\n@media (prefers-reduced-motion: reduce) {\r\n  *,\r\n  *::before,\r\n  *::after {\r\n    animation-duration: 0.01ms !important;\r\n    animation-iteration-count: 1 !important;\r\n    transition-duration: 0.01ms !important;\r\n    scroll-behavior: auto !important;\r\n  }\r\n}\r\n","sass/base/_typography.scss":"@use \"../abstracts\" as variables;\r\n\r\n/*------------------------------*\\\r\n    #typography\r\n\\*------------------------------*/\r\n\r\nhtml {\r\n  font-size: 100%;\r\n}\r\n\r\nbody {\r\n  font-size: variables.$font-primary;\r\n  font-family: variables.$font-primary;\r\n}\r\n","sass/base/_helper.scss":"/*------------------------------*\\\r\n    #helpers\r\n\\*------------------------------*/\r\n\r\n.container {\r\n  padding: 0 1.25rem 0 1.5rem;\r\n}\r\n","sass/layout/_todo.scss":"@use \"../abstracts\" as v;\r\n\r\n/*------------------------------*\\\r\n    #todo\r\n\\*------------------------------*/\r\n\r\n.todo {\r\n  &__header {\r\n    width: 100%;\r\n    background: var(--header-bg-image) top center / cover no-repeat;\r\n    object-fit: cover;\r\n  }\r\n\r\n  &__header-wrapper {\r\n    padding-top: 2.6rem;\r\n  }\r\n\r\n  &__items-wrapper {\r\n    @include v.flex-container();\r\n    margin-bottom: 2rem;\r\n  }\r\n\r\n  &__logo {\r\n    font-size: 1.6rem;\r\n    letter-spacing: 0.75rem;\r\n    font-weight: 500;\r\n    color: var(--text-color);\r\n  }\r\n\r\n  &__theme-switch {\r\n    width: 1.4rem;\r\n    height: 2rem;\r\n    background: url(\"../assets/images/icon-sun.svg\") center / contain no-repeat;\r\n  }\r\n\r\n  &__form {\r\n    display: inline-block;\r\n    margin-bottom: 2.45rem;\r\n    border-radius: 1rem;\r\n    border: none;\r\n    width: 100%;\r\n    border-radius: 0.5rem;\r\n    overflow: hidden;\r\n  }\r\n\r\n  &__input-wrapper {\r\n    display: flex;\r\n  }\r\n\r\n  &__circle {\r\n    width: 4rem;\r\n    background-color: var(--list-bg);\r\n    position: relative;\r\n\r\n    &-fill {\r\n      position: absolute;\r\n      top: 50%;\r\n      left: 50%;\r\n      transform: translate(-50%, -50%);\r\n      width: 1.2rem;\r\n      height: 1.2rem;\r\n     